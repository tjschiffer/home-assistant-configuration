nessUp ? Math.min(brightness + 5, 100) :\n        Math.max(brightness - 5, 5);\n        \n    if (isBrightnessUp && newBrightness >= 100) {\n        flow.set('isBrightnessUp', false);\n    } else if (!isBrightnessUp && newBrightness <= 5) {\n        flow.set('isBrightnessUp', true);\n    }\n\n    flow.set('lightBrightnessPct', newBrightness);\n    node.send({\n        payload: {\n            service: 'turn_on',\n            entityId: targetEntityId,\n            brightness_pct: newBrightness,\n            transition: 0.2\n        }\n    });\n}\n\nconst clearIntervals = () => {\n    const intervals = flow.get('intervals');\n    if (intervals) {\n        intervals.forEach(interval => {\n            clearInterval(interval);\n        });\n    }\n    flow.set('intervals', []);\n};\nclearIntervals();\n\nconst clearTimeouts = () => {\n    const timeouts = flow.get('timeouts');\n    if (timeouts) {\n        timeouts.forEach(timeouts => {\n            clearTimeout(timeouts);\n        });\n    }\n    flow.set('timeouts', []);\n}\nclearTimeouts();\n\nconst addToTimeouts = (timeout) => {\n    const timeouts = flow.get('timeouts') || [];\n    flow.set('timeouts', [...timeouts, timeout]);\n}\n\nif (switchState === 'on') {\n    addToTimeouts(setTimeout(() => {\n        if (flow.get('switchState') !== 'on') {\n            return;\n        }\n\n        flow.set('isHolding', true);\n        const brightnessPct = flow.get('lightBrightnessPct');\n        const isBrightnessUp = brightnessPct < 70;\n        flow.set('isBrightnessUp', isBrightnessUp);\n        updateBrightness();\n        const interval = setInterval(() => {\n            updateBrightness();\n\n            if (flow.get('switchState') !== 'on') {\n                clearIntervals();\n                clearInterval(interval);\n            }\n        }, 200);\n        flow.set('intervals', [...(flow.get('intervals') || []),\n            interval\n        ]);\n    }, 400))\n\n    return null;\n}\n\nif (flow.get('isHolding')) {\n 